<Window x:Class="ItemApp.DeleteWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ItemApp"
        mc:Ignorable="d"
        Title="DeleteWindow" Height="199" Width="229">
    <Grid>
        <TextBox x:Name="uxItemNumber" HorizontalAlignment="Center" Margin="0,49,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="119"/>
        <TextBlock HorizontalAlignment="Center" Margin="0,12,0,0" Text="Which Item # would you like to delete?" TextWrapping="Wrap" VerticalAlignment="Top"/>
        <Button x:Name="uxSubmit"  MinWidth="10" HorizontalAlignment="Center" Click="uxSubmit_Click" Margin="0,92,0,52" Width="85" >Submit</Button>

    </Grid>
</Window>
